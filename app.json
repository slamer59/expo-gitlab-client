{
  "expo": {
    "name": "Gitlalchemy",
    "slug": "expo-gitlab-client",
    "owner": "teepee",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-1.5x.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thomas.pedot.expogitlabclient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-1024x1024.png",
        "backgroundColor": "#fff"
      },
      "versionCode": 7,
      "package": "com.thomas.pedot.expogitlabclient",
      "googleServicesFile": "./google-services.json"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      ["expo-router"],
      "expo-localization",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09fe094d-696d-4e2b-ba6f-ccbf301b1ac8"
      }
    }
  }
}
